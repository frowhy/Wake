<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.frowhy.wake">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity android:name=".WakeActivity">
            <intent-filter>
                <data android:scheme="wake" />
                <data android:scheme="mqqwpa" />
                <data android:scheme="taobao" />
                <data android:scheme="weixin" />
                <data android:scheme="doubanradio" />
                <data android:scheme="zhihu" />
                <data android:scheme="alipay" />
                <data android:scheme="youku" />
                <data android:scheme="openapp.jdmoble" />
                <data android:scheme="bilibili" />

                <!--QQ空间-->
                <data android:scheme="mqzone" />
                <data android:scheme="mqzonev2" />
                <data android:scheme="mqzonev3" />
                <data android:scheme="mqzonex" />
                <data android:scheme="mqq" />
                <data android:scheme="mqzonewx" />
                <data android:scheme="mqzoneqq" />
                <data android:scheme="qqpassport" />

                <!--京东-->
                <data android:scheme="openApp.jdMobile" />
                <data android:scheme="openapp.jdmobile" />
                <data android:scheme="jingdong" />

                <!--天猫-->
                <data android:scheme="tmall" />
                <data android:scheme="itmall" />

                <!--豆瓣-->
                <data android:scheme="douban" />

                <!--百度贴吧-->
                <data android:scheme="tbmaintab" />
                <data android:scheme="invoke_frs" />
                <data android:scheme="tbfrs" />
                <data android:scheme="tbphotolive" />
                <data android:scheme="tbpb" />
                <data android:scheme="tieba_sign" />

                <!--百度云-->
                <data android:scheme="baiduyun" />

                <!--fireball-->
                <data android:scheme="fireball" />

                <!--otpAuth-->
                <data android:scheme="otpauth" />

                <!--GPlus-->
                <data android:scheme="gplus" />

                <!--Google-->
                <data android:scheme="assistant-handoff" />
                <data android:scheme="assistant-settings" />
                <data android:scheme="app" />
                <data android:scheme="android-app" />

                <!--InboxApp-->
                <data android:scheme="indexapp" />

                <!--Gmail-->
                <data android:scheme="gmail2from" />
                <data android:scheme="feedback" />

                <!--GooglePlayMusic-->
                <data android:scheme="googleplaymusic" />

                <!--youtube-->
                <data android:scheme="vnd.youtube" />
                <data android:scheme="vnd.youtube.launch" />

                <!--GoogleMaps-->
                <data android:scheme="google.maps.taxi" />
                <data android:scheme="googlemapstaxi" />
                <data android:scheme="peterparker" />
                <data android:scheme="geo.replay" />
                <data android:scheme="google.navigation" />
                <data android:scheme="google.maps" />
                <data android:scheme="google.streetview" />
                <data android:scheme="google.maps.timeline" />

                <!--GoogleEarth,GoogleMaps-->
                <data android:scheme="geo" />

                <!--GoogleTranslate-->
                <data android:scheme="googletranslate" />

                <!--GooglePlayGame-->
                <data android:scheme="version" />

                <!--Google报亭-->
                <data android:scheme="currents" />
                <data android:scheme="magazines" />
                <data android:scheme="newsstand" />
                <data android:scheme="google_newsstand_sponsored" />
                
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
